{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "ALLOWED_ORIGINS": "https://med-bridge-tawny.vercel.app"
    }
  } 